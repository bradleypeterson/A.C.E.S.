<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>A.C.E.S - @ViewData["Title"]</title>

    <script src="https://cdn.jsdelivr.net/npm/chart.js@2.9.3/dist/Chart.min.js"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/assets\fontawesome-free-5.12.0-web\css\all.min.css" asp-append-version="true" />
</head>
<body>
    <partial name="Components/Nav" />
    <main id="app-main" role="main">
        <partial name="Components/Header" />
        <article id="app-content">
            @RenderBody()
        </article>
    </main>

    @RenderSection("Scripts", required: false)
</body>
</html>
